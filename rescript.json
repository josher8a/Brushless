{
  "name": "brushless",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "gentypeconfig": {
    "language": "typescript",
    "generatedFileExtension": ".res.ts",
    "module": "es6",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".res.js",
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "bs-dependencies": [
    "@rescript/core"
  ]
}