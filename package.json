{
  "name": "brushless",
  "version": "0.1.4",
  "description": "DynamoDB utils lib write in Rescript and Typescript",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "AWS",
    "Dynamodb",
    "expressions",
    "Rescript",
    "Typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josher8a/brushless.git"
  },
  "bugs": {
    "url": "https://github.com/josher8a/brushless/issues"
  },
  "homepage": "https://github.com/josher8a/brushless",
  "scripts": {
    "build": "tsup",
    "res:build": "rescript",
    "res:clean": "rescript clean",
    "res:dev": "rescript build -w",
    "res:format": "rescript format -all",
    "test": "bun test \"src/\""
  },
  "author": {
    "name": "Joshua Hernandez",
    "email": "josher8a@gmail.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.693.0",
    "@aws-sdk/util-dynamodb": "^3.693.0",
    "@types/bun": "^1.1.13",
    "@types/node": "^18.11.9",
    "bun": "^1.1.34",
    "rescript": "12.0.0-alpha.4",
    "typescript": "^5.6.3",
    "tsup": "^8.3.5"
  }
}
